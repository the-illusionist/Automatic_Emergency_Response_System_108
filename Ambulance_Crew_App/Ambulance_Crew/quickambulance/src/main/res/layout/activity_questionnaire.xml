            android:text="Question"
            android:id="@+id/textView3"
            android:layout_below="@+id/textView2"
            android:layout_alignBottom="@+id/switch3"
            android:layout_alignTop="@+id/switch3"
            android:layout_alignParentLeft="true"
            android:layout_toLeftOf="@+id/switch3"
            android:layout_toStartOf="@+id/switch3" />
        <Switch
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/switch4"
            android:layout_below="@+id/switch3"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Question"
            android:id="@+id/textView4"
            android:layout_below="@+id/textView3"
            android:layout_alignBottom="@+id/switch4"
